{
  "name": "cohface_motion",
  "results_dir": "results",
  "datasets": [
    {
      "name": "COHFACE",
      "roi": {
        "chest": {
          "mp_complexity": 1,
          "skip_rate": 10
        }
      }
    }
  ],
  "methods": [
    "OF_Model",
    "DoF",
    "profile1D"
  ],
  "eval": {
    "win_size": 30,
    "stride": 1,
    "track_std_min_bpm": 0.3,
    "track_unique_min": 0.05,
    "track_saturation_max": 0.15
  },
  "gating": {
    "profile": "diagnostic-relaxed"
  },
  "report": {
    "output": "reports/cohface_motion",
    "unique_window": false
  },
  "steps": ["estimate", "evaluate", "metrics"]
}
